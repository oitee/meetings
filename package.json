  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oitee/meetings.git"
  },
  "author": "Oitihjya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oitee/meetings/issues"
  },
  "homepage": "https://github.com/oitee/meetings#readme",
  "devDependencies": {
    "jest": "^27.3.1"
  },
  "type": "module",
  "dependencies": {
    "pg": "^8.7.3",
    "uuid": "^8.3.2"
  },
  "jest": {
    "testMatch": [
      "**/test/**/**.js"
    ],
    "testTimeout": 60000
  }
